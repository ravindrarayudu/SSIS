          <DTS:Variables />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"
DECLARE @PackageID BIGINT
SELECT @PackageID = PackageID FROM Config.PackageInformation WHERE PackageName= '" + @[System::PackageName]  + "'
UPDATE Config.AuditPackageInformation SET LoadStatus = 'Failed' WHERE PackageID = @PackageID AND LoadStatus = 'Active'

 
DECLARE @TableID BIGINT
SELECT @TableID = TableID FROM Config.TableInformation WHERE TableName = '" + @[User::StagingTable]   + "' AND TableSchema = '"  + @[User::StagingSchema]  +  "'
UPDATE Config.AuditTableInformation SET LoadStatus = 'Failed' WHERE TableID = @TableID AND PackageID = @PackageID AND LoadStatus = 'Active'

UPDATE Config.PackageInformation SET PackageStatus='Failed' WHERE PackageID = @PackageID 


UPDATE Config.ETLBatch SET BatchStatus='Failed' WHERE BatchStatus='Active'
 "</DTS:PropertyExpression>
